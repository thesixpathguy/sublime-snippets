	<content><![CDATA[
void dfs(vector<vector<lint>> &adj,vector<lint> &visited,lint s)
{
    if(visited[s])
        return;
    visited[s]=1;
    for(auto u:adj[s])
    {
        dfs(adj,visited,u);
        
    }
}
]]></content>
